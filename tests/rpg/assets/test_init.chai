def main() {
    app.settings.mod("app_width", 960); 
    app.settings.mod("app_height", 540);
    app.settings.mod("app_title", "SQEE RPG (test_init.chai)");

//    app.settings.mod("shadQlty", 0);
//    app.settings.mod("shadFltr", 0);
//    app.settings.mod("ssaoMode", 0);
//    app.settings.mod("hdrbMode", 0);
//    app.settings.mod("fxaaMode", 0);
//    app.settings.mod("vignMode", 0);

    app.refresh();

    var cellSponza = world.add_cell("Sponza");
    cellSponza.position = Vec3F(0.0, 0.0, 0.0);
    cellSponza.load_from_file("Sponza/Main");
    cellSponza.enabled = true;
    cellSponza.refresh();

    world.skybox.enabled = true;
//    world.skybox.saturation = 1.75;
//    world.skybox.brightness = -0.2;
//    world.skybox.contrast = 0.5f;
    world.skybox.saturation = 1.0;
    world.skybox.brightness = 0.0;
    world.skybox.contrast = 1.0;
    world.skybox.alpha = 1.0;
    world.skybox.texture = "Miramar";
    world.skybox.refresh();

    world.ambient.enabled = true;
    world.ambient.colour = Vec3F(0.4, 0.4, 0.4);
//    world.ambient.colour = Vec3F(0.4, 0.35, 0.35);
//    world.ambient.colour = Vec3F(0.3, 0.3, 0.33);
    world.ambient.refresh();

    world.skylight.enabled = true;
//    world.skylight.direction = vec3(0.0, 0.0, -1.0);
    world.skylight.direction = Vec3F(-0.355, -0.465, -0.815);
//    world.skylight.colour = Vec3F(0.35, 0.35, 0.38);
    world.skylight.colour = Vec3F(0.9, 0.9, 0.9);
    world.skylight.refresh();

    world.reload_list();
    renderer.reload_lists();

    app.console.print("Welcome to SQEE!");
}


