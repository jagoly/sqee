def main() {
    var cellSponza = world.add_cell("Sponza", vec3(0.0f, 0.0f, 0.0f));
    cellSponza.load_from_file("Sponza/New");

    var modelPerson = cellSponza.add_ModelStatic("Person");
    modelPerson.pos = vec3(-0.6f, -4.0f, 5.0f);
    modelPerson.rot = vec3(0.0f, 0.0f, -20.0f);
    modelPerson.shadow = true;
    modelPerson.render = true;
    modelPerson.mPath = "Characters/Don";
    modelPerson.sPath = "Characters/Don";
    modelPerson.refresh();
    
    world.enable_cell("Sponza");
    graph.update_from_world();

    world.skybox.set_colour(vec4(1.0f, 1.0f, 1.0f, 1.0f));
    world.skybox.set_texture("cloudy");
    world.skybox.enabled = true;

    world.ambient.set_colour(vec3(0.4f, 0.4f, 0.4f));
    world.ambient.enabled = true;

    world.skylight.set_colour(vec3(0.9f, 0.9f, 0.9f));
    world.skylight.set_direction(vec3(-0.36f, -0.46f, -0.81f));
    world.skylight.enabled = true;

    console.print("Welcome to SQEE!");
}

main();
