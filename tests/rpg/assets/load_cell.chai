def load_cell() {

{ var eDiceGroup = world.root.add_child("DiceGroup");
  { var cTransform = eDiceGroup.add_TransformComponent();
    cTransform.position = Vec3F(0.0f, 0.0f, 5.0f);
    cTransform.rotation = QuatF(0.1f, 0.4f, 0.0f);
    cTransform.scale = 0.8f;
  }
  { var eDiceA = eDiceGroup.add_child("DiceA");
    { var cTransform = eDiceA.add_TransformComponent();
      cTransform.position = Vec3F(-0.2f, 1.3f, 0.6f);
      cTransform.rotation = QuatF(0.0f, 0.0f, 0.125f);
      var cModel = eDiceA.add_ModelComponent();
      cModel.scale = Vec3F(1.0f, 1.0f, 2.0f);
      cModel.mesh = "Test/Dice";
      cModel.skin = "Test/Dice";
  } }
  { var eDiceB = eDiceGroup.add_child("DiceB");
    { var cTransform = eDiceB.add_TransformComponent();
      cTransform.position = Vec3F(-1.2f, 0.6f, 0.6f);
      var cModel = eDiceB.add_ModelComponent();
      cModel.mesh = "Test/Dice";
      cModel.skin = "Test/Dice";
      cModel.shadow = false;
  } }
  { var eSpotA = eDiceGroup.add_child("SpotA");
    { var cTransform = eSpotA.add_TransformComponent();
      cTransform.position = Vec3F(0.0f, 0.0f, 2.0f);
      cTransform.scale = 2.0f;
      var cSpotLight = eSpotA.add_SpotLightComponent();
      cSpotLight.angle = 0.07f;
      cSpotLight.texsize = 128u;
  } }
  { var eSpotB = eDiceGroup.add_child("SpotB");
    { var cTransform = eSpotB.add_TransformComponent();
      cTransform.position = Vec3F(4.0f, 0.0f, 5.0f);
      cTransform.rotation = QuatF(0.0f, 0.3f, 0.0f);
      var cSpotLight = eSpotB.add_SpotLightComponent();
      cSpotLight.angle = 0.02f;
      cSpotLight.texsize = 32u;
  } }
}

}
