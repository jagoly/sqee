def add_ModelSimple(Cell _cell, String _name, bool _render, bool _shadow, bool _decals, String _mesh, String _skin, Vec3F _position, QuatF _rotation, Vec3F _scale) {
    e = Entitiy("ModelSimple", _name);
    mc = ModelComponent();
    mc.render = _render; 
    mc.shadow = _shadow; 
    mc.decals = _decals;
    mc.mesh = _mesh; 
    mc.skin = _skin;
    e.add(mc); 
    tc = TransformComponent();
    tc.position = _position;
    tc.rotation = _rotation;
    tc.scale = _scale;
    e.add(tc);
}

