class CellSponzaStuff
{
    def CellSponzaStuff()
    {
        this.musicID = -1;
    }

    def on_load_finish()
    {
        var sys_sound := world.get_SoundSystem();

        this.musicID = sys_sound.play("music/MusicA", 2u);
        sys_sound.set_sound_loop(this.musicID, true);
    }
};

